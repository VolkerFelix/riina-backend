  "query": "\n                UPDATE games \n                SET home_score = $1, \n                    away_score = $2, \n                    status = 'finished',\n                    winner_team_id = CASE \n                        WHEN $3 = 'home_team_id' THEN home_team_id\n                        ELSE away_team_id\n                    END,\n                    updated_at = NOW()\n                WHERE id = $4\n                RETURNING *\n                ",
