{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            p.id, p.user_id, p.post_type as \"post_type: String\",\n            p.content, p.workout_id, p.image_urls, p.video_urls,\n            p.ad_metadata, p.visibility as \"visibility: String\",\n            p.is_editable, p.created_at, p.updated_at, p.edited_at,\n            u.username, u.profile_picture_url,\n            COALESCE((SELECT COUNT(*) FROM post_reactions wr WHERE wr.workout_id = p.workout_id), 0) as reaction_count,\n            COALESCE((SELECT COUNT(*) FROM post_comments wc WHERE wc.workout_id = p.workout_id), 0) as comment_count,\n            EXISTS(SELECT 1 FROM post_reactions wr WHERE wr.workout_id = p.workout_id AND wr.user_id = $2) as user_has_reacted\n        FROM posts p\n        JOIN users u ON u.id = p.user_id\n        WHERE p.id = $1\n        ",
